generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Product {
  id          Int      @id @default(autoincrement())
  quantity    Int
  sku         String
  description String
  store       Int
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}

model Store {
  id        Int      @id @default(autoincrement())
  name      String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
